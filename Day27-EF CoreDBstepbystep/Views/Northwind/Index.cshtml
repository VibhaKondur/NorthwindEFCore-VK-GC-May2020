
@{
    ViewData["Title"] = "Index";
}

@model List<Customers>
<h1>Index</h1>

<table class="table table-success">
    <tr>
        <th>Company Name</th>
        <th>City</th>
        <th>Phone Number</th>
    </tr>
    @foreach (var customer in Model)
    {
        <tr>
            <td>@customer.CompanyName</td>
            <td>@customer.City</td>
            <td>@customer.Phone</td>
        </tr>
    }
</table>

<a href="/Northwind/CustomerForm">Gooooo add a customer!</a>

